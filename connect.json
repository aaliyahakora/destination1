{
  "baseUrl": "https://567c976a.ngrok.io",
  "key": "connect-wiki",
  "modules": {
    "repoPages": [
      {
        "key": "wiki",
        "location": "org.bitbucket.repository.navigation",
        "name": {
          "value": "Wiki"
        },
        "url": "/?repoUuid={repository.uuid}&repo={repository.full_name}&repoScm={repository.scm}&settings={repository.properties.get(attribute=app.key).settings}&userUuid={user.uuid}&userName={user.username}&appKey={app.key}&userIsAdmin={repository.has_permission(permission=\"admin\")}"
      }
    ]
  },
  "name": "Connect Wiki",
  "description": "Render bitbucket wikis with connect.",
  "scopes": [
    "repository"
  ],
  "contexts": [
    "account"
  ]
}
